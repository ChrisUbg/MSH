[Unit]
Description=MSH Docker Services
Requires=docker.service
After=docker.service
Wants=network-online.target
After=network-online.target

[Service]
Type=oneshot
RemainAfterExit=yes
WorkingDirectory=/home/chregg/msh
ExecStart=/usr/bin/docker-compose -f docker/docker-compose-all.yml up -d
ExecStop=/usr/bin/docker-compose -f docker/docker-compose-all.yml down
TimeoutStartSec=0

[Install]
WantedBy=multi-user.target 